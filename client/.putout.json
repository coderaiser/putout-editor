{
    "match": {
        "src/components/PasteDropTarget.js": {
            "remove-unused-variables": "off"
        },
        "src/core/ParseResult.js": {
            "remove-unused-variables": "off"
        },
        "src/parsers/js/recast.js": {
            "remove-unused-variables": "off"
        },
        ".eslintrc.js": {
            "remove-console": "on"
        },
        "*.cjs": {
            "nodejs/convert-esm-to-commonjs": "on"
        }
    },
    "rules": {
        "remove-console": "off",
        "remove-empty/import": "off",
        "apply-optional-chaining": "on",
        "nodejs/convert-commonjs-to-esm/commons": "off"
    }
}
